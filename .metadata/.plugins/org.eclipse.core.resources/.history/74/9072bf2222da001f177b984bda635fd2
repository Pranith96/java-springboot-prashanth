		Student s1 = new Student();
		s1.setId(1);
		s1.setAge(13);
		s1.setName("ABC");
		s1.setStatus("ACTIVE");

		list.add(s1);
		list.add(new Student(2, "XYZ", 20, "ACTIVE"));
		list.add(new Student(2, "PQR", 20, "InACTIVE"));

		
