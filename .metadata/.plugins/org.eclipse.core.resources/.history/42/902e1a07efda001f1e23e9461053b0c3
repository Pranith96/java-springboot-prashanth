
public class Example {

	public void display(String str) {
		if (str != null && str.isEmpty()) {
			str = str.toUpperCase();
			System.out.println(str);
		}
	}
	
	public void display(String str1) {
		if (str != null && str.isEmpty()) {
			str = str.toUpperCase();
			System.out.println(str);
		}
	}
}
